<%-include("./partials/header")%>

<div class="container">
	
		<h2 style="margin-top:100px;"class="text-center">Edit form</h2>
		<div style="width:30%;margin:20px auto;">
			<form class="" method="POST" action="/user/<%=url%>/edit">
				<div class="form-group">
					<input class="form-control" type="text" name="olddata[link]" placeholder="Previous Gdrive link">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="newdata[title]" placeholder="New Title">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="newdata[link]" placeholder="Gdrive Link">
				</div>
				<div class="form-group">
					<button class="btn btn-primary btn-block">Edit</button>
				</div>
			</form>
			<a href='/user/<%=url%>'>Go Back</a>
		</div>
</div>



<%-include("./partials/footer")%>